{
  "name": "web-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^17.0.45",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "axios": "^1.7.2",
    "express": "^4.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-gtm-module": "^2.0.11",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.25.1",
    "react-scripts": "5.0.1",
    "react-yandex-metrika": "^2.6.0",
    "styled-components": "^6.1.12",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "concurrently \"npm run dev\" \"npm run stub\"",
    "build": "ENV_MODE=prod craco build",
    "dev": "ENV_MODE=dev craco start",
    "lint": "eslint . --ext .ts,.tsx",
    "stub": "concurrently \"npm run database\" \"npm run server\"",
    "server": "node ./stub/index.js",
    "database": "json-server -w stub/database/db.json -p 3001",
    "nginx:start": "./nginx/nginx -p ./nginx",
    "nginx:stop": "./nginx/nginx -p ./nginx -s stop",
    "nginx:check": "cat ./nginx/logs/nginx.pid"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@types/react-gtm-module": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "concurrently": "^8.2.2",
    "craco-alias": "^3.0.1",
    "dotenv": "^16.4.5",
    "eslint": "8.57.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "globals": "^15.8.0",
    "json-server": "0.17.4",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "stylelint": "14.16.1",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  }
}
